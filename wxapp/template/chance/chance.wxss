.chance {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 55;
}

.chance__mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,.6);
}

.chance__con {
    padding: 25rpx 20rpx 0rpx;
    width: 650rpx;
    background: #db5a3a;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    border-radius: 20rpx;
}

.chance__tit {
    font-size: 40rpx;
    color: #fff45c;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 25rpx;
}

.chance__tit.goagain {
    margin-bottom: 10rpx;
}

.chance__tit image {
    width: 79rpx;
    height: 80rpx;
    margin-right: 22rpx;
}

.chance__tit view {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.chance__tit view text:first-child {
    margin-bottom: 5rpx;
}

.chance__list {
    width: 543rpx;
    height: 92rpx;
    padding: 30rpx 50rpx;
    display: flex;
    flex-direction: column;
    margin-bottom: 15rpx;
}

.chance__list.share {
    box-sizing: content-box;
    line-height: 45rpx;
}

.chance__list--num {
    font-size: 35rpx;
    color: #db5a3a;
    font-weight: 700;
    display: flex;
    justify-content: space-between;
    margin-bottom: 10rpx;
}

.chance__list--num.orange {
    color: #fbc600;
}

.chance__list--rate {
    font-size: 30rpx;
    color: #db5a3a;
    display: flex;
    justify-content: space-between;
}

.chance__list--rate>view {
    padding: 0 10rpx;
}

.chance__list--rate .del {
    position: relative;
}

.chance__list--rate .del view {
    width: 100%;
    height: 3rpx;
    background: #db5a3a;
    position: absolute;
    top: 51%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.chance__close {
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    top: -30rpx;
    right: -30rpx;
}

.chance__close image {
    width: 60rpx;
    height: 60rpx;
}

.chance__goagain {
    width: 100%;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-bottom: 10rpx;
}

.chance__goagain--line {
    height: 5rpx;
    width: 100%;
    position: absolute;
    top: 50%;
    left: 0;
    transform: translate(0,-50%);
    background: #fff45c;
}

.chance__goagain button {
    font-size: 40rpx;
    color: #fff45c;
    display: flex;
    width: 210rpx;
    justify-content: center;
    background: #db5a3a;
    position: relative;
    z-index: 22;
    height: 60rpx;
    padding: 0;
    line-height: 1.3;
}

.chance__goagain button[plain] {
    border: 0;
}

.pack {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.pack__con {
    width: 699rpx;
    height: 427rpx;
    padding-top: 300rpx;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 699rpx 727rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.pack__con--fee {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    color: #a8520d;
    font-size: 35rpx;
    margin-bottom: 20rpx;
}

.pack__con--share {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    color: #a8520d;
    font-size: 35rpx;
    margin-bottom: 15rpx;
}

.pack__con--btn {
    width: 280rpx;
    height: 85rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 35rpx;
    color: #FFF;
    background: #e32622;
    border-radius: 30rpx;
    margin: 0 180rpx 0 220rpx;
}

.pack__con--btn image {
    width: 19rpx;
    height: 32rpx;
    margin-left: 20rpx;
}