@import "../../template/ruler/ruler.wxss";

#revive {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 750rpx;
}

.revive {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 999;
}

.revive__ad {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 750rpx;
}

.revive__mask {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background: rgba(0,0,0,0.6);
}

.revive__con {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 600rpx;
    padding-top: 60rpx;
    border-radius: 15rpx;
    overflow: hidden;
    background: #db5a3a;
    z-index: 333;
}

.revive__info {
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 26rpx;
    color: #fff;
    margin-bottom: 30rpx;
}

.revive__info text {
    margin-bottom: 10rpx;
}

.revive__btn {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 600rpx;
    height: 195rpx;
    background: #FFF;
}

.revive__btn button {
    display: flex;
    justify-content: center;
    align-items: center;
    min-width: 285rpx;
    height: 90rpx;
    font-size: 36rpx;
    border-radius: 45rpx;
    color: #fff;
    background: #db5a3a;
}

.revive__close {
    position: absolute;
    top: 0;
    right: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 70rpx;
    height: 70rpx;
}

.revive__close image {
    width: 60rpx;
    height: 60rpx;
}

.float {
    position: fixed;
    top: 250rpx;
    right: 0;
    z-index: 33;
}

.float__go {
    display: flex;
    width: 130rpx;
}

.float__go image {
    width: 130rpx;
}

.index {
    background: #de613f;
    min-height: 100vh;
}

.index__load {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 750rpx;
    height: 100vh;
    background: #de613f;
}

.index__load image {
    width: 498rpx;
    height: 144rpx;
}

.index__head {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-right: 30rpx;
    padding-top: 40rpx;
}

.index__rule {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100rpx;
    height: 41rpx;
    border: 3rpx solid #fff;
    border-radius: 22rpx;
    font-size: 30rpx;
    color: #fff;
}

.index__logo {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 35rpx auto 45rpx;
}

.index__logo image {
    width: 604rpx;
}

.index__logo image:last-child {
    width: 586rpx;
    height: 84rpx;
    margin-top: 15rpx;
}

.index__btn::after {
    border: none;
}

.index__btn {
    width: 639rpx;
    height: 109rpx;
    margin: 60rpx auto 0;
    background: url("data:image/png;base64,") 0 0/639rpx 109rpx no-repeat;
}

.index__ad {
    width: 685rpx;
    margin: 100rpx auto 0;
}

.index__more {
    display: flex;
    justify-content: space-between;
    width: 685rpx;
}

.index__more--list {
    flex: 1;
    display: flex;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
    height: 148rpx;
}

.index__more--list image {
    display: block;
    border-radius: 10rpx;
    width: 184rpx;
    height: 148rpx;
}

.index__share {
    position: relative;
    display: flex;
    justify-content: center;
    background: url("data:image/png;base64,") 0 0/155rpx 78rpx no-repeat;
}

.index__share button {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 155rpx;
    height: 78rpx;
    padding: 0;
    border-radius: 0;
}

.index__share button[plain] {
    padding: 0;
    border: 0;
    color: #fff;
}

.index__nav {
    display: flex;
    width: 690rpx;
    height: 342rpx;
    padding: 0 30rpx;
    margin-top: 80rpx;
    background: url('data:image/png;base64,') 0 0/750rpx 342rpx no-repeat;
}

.index__nav view {
    flex: 1;
    display: flex;
    justify-content: center;
    height: 165rpx;
    transform: translate(0,-34rpx);
}

.index__nav view image {
    width: 122rpx;
    height: 165rpx;
}

.index__liu {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
}

.index__liu image {
    position: fixed;
    bottom: 0;
    width: 100%;
}

.index_more {
    width: 454rpx;
    height: 87rpx;
    margin: 40rpx auto;
    text-align: center;
    line-height: 70rpx;
    color: white;
    background: url('data:image/png;base64,') 0 0/contain no-repeat;
}

.revive_title {
    color: #ffd302;
}

.revive__img {
    margin-top: 50rpx;
    display: flex;
    font-size: 36rpx;
    color: white;
    align-items: center;
}

.revive__tit {
    margin: 40rpx 0 10rpx;
    font-size: 28rpx;
    color: white;
}

.revive__t {
    font-size: 28rpx;
    color: #ffd302;
    margin-bottom: 40rpx;
}

.revive_nochence {
    color: white;
    margin-bottom: 40rpx;
}

.revive_red {
    color: white;
    margin: 30rpx 0 40rpx;
}